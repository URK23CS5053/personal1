5.create new fork
git clone gitrepo
cd gitrepo
git remote -v
git remote add upstream https...old repo
git remote -v
git fetch upstream
git branch
git merge upstream/main
git push origin main

6.cd repo
git init
git status
cd gitrepo
git submodule add https...
git status
git add -A
git commit -m "submodule"
git push -u origin main
git submodule deinit -f gitrepo
git config --global alias.st status
git config --global alias.br branch

